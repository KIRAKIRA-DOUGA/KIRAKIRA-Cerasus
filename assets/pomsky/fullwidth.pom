# 匹配是否是全宽字符，但是去除谚文部分（因为韩语使用半宽标点）。

[
	'〈〉'  # 尖括号
	'⺀'-'ㄯ'  # 偏旁部首
	'㆐'-'ㇿ'  # 古日语
	'㈠'-'㉇'  # 带括弧汉字
	'㊀'-'㊰'  # 带圈汉字
	'㋀'-'㋋'  # 月份
	'㋐'-'㍰'  # 小时
	'㍻'-'㍿'  # 日本年号
	'㏠'-'㏾'  # 日期
	'㐀'-'䶿'  # 汉字扩展 A
	'一'-'鿿'  # 汉字基本区
	'ꀀ'-'꓆'  # 彝文
	'豈'-'龎'  # 汉字兼容区
	'︐'-'︙'  # 竖排标点
	'︰'-'﹫'  # 竖排兼容标点
	'！'-'｠'  # 全宽区
	'￠'-'￦'  # 全宽货币符号
	'𚿰'-'𛅧'  # 假名扩展区
	'🈀'-'🉑'  # 带圈或括弧汉字增补
	'𠀀'-'𿿽'  # 汉字扩展第二 ~ 三平面
]
