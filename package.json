		"@vueuse/gesture": "^2.0.0",
		"@vueuse/head": "^2.0.0",
		"@vueuse/nuxt": "^11.0.3",
		"axios": "^1.7.5",
		"css-doodle": "0.40.0",
		"danmaku": "^2.0.6",
		"gsap": "^3.12.5",
		"ipx": "^3.0.1",
		"js-confetti": "^0.12.0",
		"lottie-web": "^5.12.2",
		"mediainfo.js": "^0.3.2",
		"mitt": "^3.0.1",
		"nuxt-icons": "^3.2.1",
		"pinia": "^2.2.2",
		"shaka-player": "^4.10.11",
		"tus-js-client": "^4.1.0",
		"vue-audio-visual": "^3.0.8",
		"vue-cropper": "^1.1.4",
		"vue-virtual-scroller": "2.0.0-beta.8"
	},
	"pnpm": {
		"patchedDependencies": {
			"lottie-web@5.12.2": "patches/lottie-web@5.12.2.patch",
			"shaka-player@4.10.11": "patches/shaka-player@4.10.11.patch",
			"@vue/compiler-sfc": "patches/@vue__compiler-sfc.patch"
		}
	},
