{
	"name": "kirakira-cerasus",
	"version": "0.0.0-alpha",
	"description": "KIRAKIRA-Cerasus, The PC/PE Front-end Created by Nuxt3.",
	"private": true,
	"type": "module",
	"module": "esnext",
	"repository": {
		"type": "git",
		"url": "https://github.com/KIRAKIRA-DOUGA/KIRAKIRA-Cerasus.git"
	},
	"homepage": "https://kirakira.moe/",
	"homepageFallback": [
		"https://cerasus.kirakira.moe/",
		"https://kira.calibur.tv/",
		"https://kirakira-cerasus.vercel.app/"
	],
	"github": "https://github.com/KIRAKIRA-DOUGA/KIRAKIRA-Cerasus",
	"bugs": "https://github.com/KIRAKIRA-DOUGA/KIRAKIRA-Cerasus/issues",
	"license": "BSD 3-Clause",
	"author": "KIRAKIRA Project Team",
	"scripts": {
		"build": "nuxt build && nuxt start",
		"dev": "cross-env nuxt dev --host --https --ssl-cert server/server.cer --ssl-key server/server.key",
		"dev-local": "cross-env VITE_BACKEND_PROVIDER=localhost nuxt dev --host --https --ssl-cert server/server.cer --ssl-key server/server.key",
		"dev-http": "cross-env nuxt dev",
		"dev-http-local": "cross-env VITE_BACKEND_PROVIDER=localhost nuxt dev",
		"dev-localhost": "cross-env nuxt dev --host localhost --https --ssl-cert server/server.cer --ssl-key server/server.key",
		"dev-local-localhost": "cross-env VITE_BACKEND_PROVIDER=localhost nuxt dev --host localhost --https --ssl-cert server/server.cer --ssl-key server/server.key",
		"dev-http-localhost": "cross-env nuxt dev --host localhost",
		"dev-http-local-localhost": "cross-env VITE_BACKEND_PROVIDER=localhost nuxt dev --host localhost",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"postinstall": "node postinstall.js",
		"lint:css": "stylelint **/*.{html,vue,css,sass,scss}",
		"typed-scss-modules": "typed-scss-modules assets/styles/*.module.scss --exportType default",
		"update-eases": "tsx scripts/update-eases.ts",
		"compact-svg": "tsx scripts/compact-svg.ts"
	},
	"lint-staged": {
		"*.{html,vue,css,sass,scss}": [
			"stylelint --fix",
			"git add"
		]
	},
	"cssnano": {
		"preset": [
			"default",
			{
				"calc": false
			}
		]
	},
	"overrides": {
		"vue": "latest"
	},
